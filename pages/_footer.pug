footer.page-footer.orange.lighten-3
  div.row.col.s12
    ul.tabs.orange.lighten-3
      //.blue
      li.tab.col.s1.right
        a.footer-tabs(href='#faqs') FAQs
      li.tab.col.s2.right
        a.footer-tabs(href='#contactus') Contact Us
      li.tab.col.s2.right
        a.active.footer-tabs Â© 2017 BusinessBook. All Right Reserved.
  #contactus.col.s12.container  
    div.row
      h3 Contact Us  
      div.col.s12(style='height: 2%')
      form(method='POST' action='/register')
        div.input-field.col-s6
          input#name(type="text") 
          label(for="name") Firstname Lastname

        div.input-field.col-s6  
          input#email(type="text")
          label(for='email') Email

        div.input-field.col-s6  
          input#email2(type="text")
          label(for='email2') Repeat Email

        div.input-field.col-s6  
          input#pw(type="text")
          label(for='pw') Password

        div.input-field.col-s6  
          input#pw2(type="text")
          label(for='pw2') Confirm Password

        a#sign-up-button.btn.waves-effect.waves-light(type="submit", name="action") Sign Up
          i.material-icons send
